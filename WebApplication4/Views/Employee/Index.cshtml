@{
}
@model List<Employee>
<h2>List Of Employees</h2>
<table class="table">
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Fisrt Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model)
        {
            <tr>
                <td>@e.Employeeid</td>
                <td>@e.FirstName</td>
                <td>@e.LastName</td>
                <td>@e.Email</td>
                <td>@e.Department</td>

            </tr>
        }
    </tbody>
</table>